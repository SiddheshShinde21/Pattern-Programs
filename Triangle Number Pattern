import java.util.*;
public class TriangleNumberPattern {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter any number:");
        int n = sc.nextInt();
        int num = 1;
        for(int i = 0; i<n; i++){
            for(int j = 0; j<i+1; j++){
                System.out.print(num);
            }
            num++;
            System.out.println("");
        }

    }
}

output:-
Enter any number:5
1
22
333
4444
55555
